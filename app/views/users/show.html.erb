<div class="container">
  <div class="row">
  <div class= "d-flex align-items-center justify-content-center">
  
  <p><%= @user.nickname %>さんの投稿一覧</p>
  <% @diaries.each do |diary| %>
    <div class="content_post" style="background-image: url(<%= diary.image %>);">
      <p><%= diary.content %></p>
      <span class="name"><%= diary.title %></span>
    </div>
  <% end %>
</div>
</div>
</div>         